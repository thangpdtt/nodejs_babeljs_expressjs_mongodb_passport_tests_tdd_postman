    "mongoose-unique-validator": "^2.0.2",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "2.0.0-beta11",
